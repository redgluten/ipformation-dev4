# Fichier EditorConfig http://editorconfig.org

# top-most EditorConfig file
root = true

# Par d√©faut
[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = tru
indent_style = space
indent_size = 4

[*.md]
trim_trailing_whitespace = false

# Indentation pour les CSS et SCSS en tab
[*.css, *.scss]
indent_style = tab
